<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yongjian.english_tranning_talk">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yongjian.english_tranning_talk.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.LoginActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.SelectActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.LRegisterActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.TRegisterActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.LDrawerActivity" android:launchMode="singleTask"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.TDrawerActivity" android:launchMode="singleTask">
        </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.TeachersActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.AssesActivity"> </activity>
        <activity android:name="com.yongjian.english_tranning_talk.activity.CommitAssesActivity"> </activity>

    </application>

</manifest>